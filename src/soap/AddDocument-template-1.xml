            <scr:OrgDate>{{payload.OrgDate}}</scr:OrgDate>

            <scr:OutsideSign>{{payload.OutsideSign}}</scr:OutsideSign>
            <scr:OutsideSignType>{{payload.OutsideSignType}}</scr:OutsideSignType>
            <scr:PayDate>{{payload.PayDate}}</scr:PayDate>
            <scr:ProcessFlag>{{payload.ProcessFlag}}</scr:ProcessFlag>
            <scr:Purpose>{{payload.Purpose}}</scr:Purpose>
            
            <scr:SourceCode>{{payload.SourceCode}}</scr:SourceCode>
            <scr:SystemId>808</scr:SystemId>
            <scr:TargetCode>{{payload.TargetCode}}</scr:TargetCode>

         </tem:documentParams>
      </tem:AddDocument>
   </soapenv:Body>
</soapenv:Envelope>