      <tem:AddDocument>
         <tem:documentParams>
            <scr:ActionId>{{payload.ActionId}}</scr:ActionId>
            <scr:BranchId>{{payload.BranchId}}</scr:BranchId>
            <scr:Code>{{payload.Code}}</scr:Code>
            
            <scr:CreditCode>{{payload.CreditCode}}</scr:CreditCode>
